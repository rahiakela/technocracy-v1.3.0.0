{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/manifest.json",
        "/*.css",
        "/*.js",
        "/*.bundle.css",
        "/*.bundle.js",
        "/*.chunk.js"
      ],
      "urls": [
        "https://fonts.googleapis.com/icon?family=Material+Icons",
        "https://jmblog.github.io/color-themes-for-google-code-prettify/themes/atelier-savanna-light.min.css"
      ]
    }
  },
  {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }
  ],
  "dataGroups": [
    {
      "name": "api-performance",
      "urls": [
        "/api/*"
      ],
      "cacheConfig":  {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d"
      }
    }
  ]
}
